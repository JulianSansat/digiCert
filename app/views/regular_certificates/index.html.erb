<p id="notice"><%= notice %></p>

<h1>Regular Certificates</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regular_certificates.each do |regular_certificate| %>
      <tr>
        <td><%= regular_certificate.subject %></td>
        <td><%= link_to "certificado", regular_certificate.ca.url(:original, false) %></td>
        <td><%= link_to "chave_publica", regular_certificate.public.url(:original, false) %></td>
        <td><%= link_to 'Show', regular_certificate %></td>
        <td><%= link_to 'Edit', edit_regular_certificate_path(regular_certificate) %></td>
        <td><%= link_to 'Destroy', regular_certificate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regular Certificate', new_regular_certificate_path %>
